{
  "name": "@comakit/cli",
  "version": "0.0.0",
  "description": "Add comakit's components to your apps",
  "type": "module",
  "main": "src/index.ts",
  "author": {
    "name": "Comaqui",
    "url": "https://github.com/comaqui"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "keywords": [
    "ui",
    "tailwind",
    "radix-ui",
    "comakit",
    "components"
  ],
  "bin": {
    "comakit": "dist/index.cjs"
  },
  "scripts": {
    "dev": "tsup --watch ./src/index.ts",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "publish:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "publish:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "publish:release": "pnpm build && pnpm publish --access public"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/revogabe/comakit.git",
    "directory": "packages/cli"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@clack/core": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "cleye": "^1.3.2",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "picocolors": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "20.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}